<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:makeramen="http://schemas.android.com/apk/res/com.whut.myMap"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <ScrollView
        android:id="@+id/fs_sv"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:fadingEdge="vertical"
        android:scrollbars="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="250dp"
                android:background="@drawable/page_cover_default_background" >

                <com.makeramen.RoundedImageView
                    xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/fs_head"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="62dp"
                    android:scaleType="centerInside"
                    android:src="@drawable/meili"
                    makeramen:border_color="@android:color/transparent"
                    makeramen:border_width="1dip"
                    makeramen:corner_radius="100dip" />

                <TextView
                    android:id="@+id/fs_username"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/fs_head"
                    android:layout_centerInParent="true"
                    android:text="" />

                <TextView
                    android:id="@+id/fs_personal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/fs_username"
                    android:layout_centerInParent="true"
                    android:textStyle="italic"
                    android:text="s" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/fs1"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:background="@drawable/layout_selector"
                android:clickable="true" >

                <ImageView
                    android:id="@+id/apl1"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_centerVertical="true"
                    android:scaleType="centerInside"
                    android:src="@drawable/icon_card_foot_blue" />

                <TextView
                    android:id="@+id/apl12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:layout_toRightOf="@id/apl1"
                    android:text="草稿箱"
                    android:textColor="#404040"
                    android:textSize="20sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    android:src="@drawable/setting_array" />

                <View
                    android:layout_width="wrap_content"
                    android:layout_height="1dp"
                    android:background="#6fcccccc" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/fs2"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:background="@drawable/layout_selector"
                android:clickable="true" >

                <ImageView
                    android:id="@+id/apl2"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_centerVertical="true"
                    android:scaleType="centerInside"
                    android:src="@drawable/icon_details_collect_normal" />

                <TextView
                    android:id="@+id/apl22"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:layout_toRightOf="@id/apl2"
                    android:text="收藏夹"
                    android:textColor="#404040"
                    android:textSize="20sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    android:src="@drawable/setting_array" />

                <View
                    android:layout_width="wrap_content"
                    android:layout_height="1dp"
                    android:background="#6fcccccc" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/fs3"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:background="@drawable/layout_selector"
                android:clickable="true" >

                <ImageView
                    android:id="@+id/apl3"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_centerVertical="true"
                    android:scaleType="centerInside"
                    android:src="@drawable/icon_track_map_bar_cluster" />

                <TextView
                    android:id="@+id/apl32"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:layout_toRightOf="@id/apl3"
                    android:text="我的评论"
                    android:textColor="#404040"
                    android:textSize="20sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    android:layout_toLeftOf="@id/apl_title"
                    android:src="@drawable/setting_array" />

                <View
                    android:layout_width="wrap_content"
                    android:layout_height="1dp"
                    android:background="#6fcccccc" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/fs4"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:background="@drawable/layout_selector"
                android:clickable="true" >

                <ImageView
                    android:id="@+id/apl4"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_centerVertical="true"
                    android:scaleType="centerInside"
                    android:src="@drawable/track_icon_timeline_foot" />

                <TextView
                    android:id="@+id/apl41"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:layout_toRightOf="@id/apl4"
                    android:text="@string/atwho"
                    android:textColor="#404040"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/apl42"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@id/apl41"
                    android:text="0"
                    android:textColor="@android:color/transparent"
                    android:textSize="20sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    android:src="@drawable/setting_array" />

                <View
                    android:layout_width="wrap_content"
                    android:layout_height="1dp"
                    android:background="#6fcccccc" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/fs5"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:background="@drawable/layout_selector"
                android:clickable="true" >

                <ImageView
                    android:id="@+id/apl5"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_centerVertical="true"
                    android:scaleType="centerInside"
                    android:src="@drawable/addpoi_record_comment" />

                <TextView
                    android:id="@+id/apl51"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:layout_toRightOf="@id/apl5"
                    android:text="我的动态"
                    android:textColor="#404040"
                    android:textSize="20sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    android:src="@drawable/setting_array" />

                <View
                    android:layout_width="wrap_content"
                    android:layout_height="1dp"
                    android:background="#6fcccccc" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/fs6"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:background="@drawable/layout_selector"
                android:clickable="true" >

                <ImageView
                    android:id="@+id/apl6"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_centerVertical="true"
                    android:scaleType="centerInside"
                    android:src="@drawable/lbspay_location_refresh_bdmap" />

                <TextView
                    android:id="@+id/apl61"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:layout_toRightOf="@id/apl6"
                    android:text="我的信息"
                    android:textColor="#404040"
                    android:textSize="20sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    android:layout_toLeftOf="@id/apl61"
                    android:src="@drawable/setting_array" />

                <View
                    android:layout_width="wrap_content"
                    android:layout_height="1dp"
                    android:background="#6fcccccc" />
            </RelativeLayout>

       
            <RelativeLayout
                android:id="@+id/fs8"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:background="@drawable/layout_selector"
                android:clickable="true" >

                <ImageView
                    android:id="@+id/apl8"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_centerVertical="true"
                    android:scaleType="centerInside"
                    android:src="@drawable/program_set" />

                <TextView
                    android:id="@+id/apl81"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:layout_toRightOf="@id/apl8"
                    android:text="设置"
                    android:textColor="#404040"
                    android:textSize="20sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    android:layout_toLeftOf="@id/apl81"
                    android:src="@drawable/setting_array" />

                <View
                    android:layout_width="wrap_content"
                    android:layout_height="1dp"
                    android:background="#6fcccccc" />
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>