<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.whut.myMap.Help" >
    
        <LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    
            <RelativeLayout
        android:id="@+id/re1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:clickable="true" >

        <TextView
            android:id="@+id/create"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:padding="7dp"
            android:text="创建游记"
            android:textSize="16sp" />
   
    </RelativeLayout>
     <View
        android:id="@+id/view3"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_below="@id/create"
        android:background="#cccccc" />
    <RelativeLayout
        android:id="@+id/re2"
        android:layout_width="match_parent"
        android:layout_below="@id/view3"
        android:layout_height="50dp"
        android:clickable="true" >

        <TextView
            android:id="@+id/edit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:padding="7dp"
            android:text="编辑游记"
            android:textSize="16sp" />
   
    </RelativeLayout>

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_below="@id/edit"
        android:background="#cccccc" />
      <RelativeLayout
        android:id="@+id/re3"
        android:layout_below="@id/view"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:clickable="true" >
    
    <TextView
             android:id="@+id/finsih"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:padding="7dp"
            android:text="分享游记"
            android:textSize="16sp" />
  </RelativeLayout>
  
        <View
        android:id="@+id/view1"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_below="@id/re3"
        android:background="#cccccc" />
    <RelativeLayout
        android:id="@+id/re4"
        android:layout_below="@id/view1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:clickable="true" >
    <TextView
             android:id="@+id/mark"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:padding="7dp"
            android:text="添加标记"
            android:textSize="16sp" />
  </RelativeLayout>
  <View
        android:id="@+id/view4"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_below="@id/re3"
        android:background="#cccccc" />
      <RelativeLayout
        android:id="@+id/re5"
        android:layout_below="@id/view1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:clickable="true" >
    <TextView
             android:id="@+id/score"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:padding="7dp"
            android:text="积分奖励制度"
            android:textSize="16sp" />
  </RelativeLayout>
  <View
        android:id="@+id/view5"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_below="@id/re3"
        android:background="#cccccc" />
      </LinearLayout>
    </ScrollView>  
