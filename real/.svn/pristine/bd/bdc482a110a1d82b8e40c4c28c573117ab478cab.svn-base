<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <RelativeLayout
        android:id="@+id/cs_rl3"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:clickable="true" 
          android:background="@drawable/layout_selector">

        <TextView
            android:id="@+id/changepwd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:padding="7dp"
            android:text="修改密码"
            android:textSize="16sp" />

        <ImageView
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:src="@drawable/setting_array" />
    </RelativeLayout>

    <View
        android:id="@+id/view3"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_below="@id/cs_rl3"
        android:background="#cccccc" />

    <RelativeLayout
        android:id="@+id/cs_rl"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@id/view3"
        android:clickable="true"
          android:background="@drawable/layout_selector" >

        <TextView
            android:id="@+id/clear"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:padding="7dp"
            android:text="清除缓存"
            android:textSize="16sp" />

        <ImageView
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:src="@drawable/setting_array" />
    </RelativeLayout>

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_below="@id/cs_rl"
        android:background="#cccccc" />

    <RelativeLayout
        android:id="@+id/cs_rl2"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@id/view"
        android:clickable="true" 
          android:background="@drawable/layout_selector">

        <TextView
            android:id="@+id/help"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:padding="7dp"
            android:text="帮助中心"
            android:textSize="16sp" />

        <ImageView
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:src="@drawable/setting_array" />
    </RelativeLayout>

    <View
        android:id="@+id/view1"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_below="@id/cs_rl2"
        android:background="#cccccc" />

    <RelativeLayout
        android:id="@+id/cs_rl1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@id/view1"
        android:clickable="true"
          android:background="@drawable/layout_selector" >

        <TextView
            android:id="@+id/aboutus"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:padding="7dp"
            android:text="关于软件"
            android:textSize="16sp" />

        <ImageView
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:src="@drawable/setting_array" />
    </RelativeLayout>

    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_below="@id/cs_rl1"
        android:background="#cccccc" />

    <Button
        android:id="@+id/exit_btn"
        android:layout_width="300dp"
        android:layout_height="50dip"
        android:layout_below="@id/view2"
        android:layout_centerHorizontal="true"
        android:background="@drawable/exit_button"
        android:padding="5dp"
        android:text="退出登录"
        android:textColor="#ffffff" />

    <ImageView
        android:layout_width="18dp"
        android:layout_height="18dp"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="10dp"
        android:src="@drawable/setting_array" />
</RelativeLayout>