package com.whut.myMap.entites;

import java.io.Serializable;

public class picture implements Serializable{
	  private int picture_id;
	   private int reds_id;
	   private String imgurl;
   public int getPicture_id() {
		return picture_id;
	}
	public void setPicture_id(int picture_id) {
		this.picture_id = picture_id;
	}
	public int getReds_id() {
		return reds_id;
	}
	public void setReds_id(int reds_id) {
		this.reds_id = reds_id;
	}
	public String getImgurl() {
		return imgurl;
	}
	public void setImgurl(String imgurl) {
		this.imgurl = imgurl;
	}
	public picture(int picture_id, int reds_id, String imgurl) {
		super();
		this.picture_id = picture_id;
		this.reds_id = reds_id;
		this.imgurl = imgurl;
	}
	public picture() {
		super();
		// TODO Auto-generated constructor stub
	}
	@Override
	public String toString() {
		return "picturebean [picture_id=" + picture_id + ", reds_id=" + reds_id
				+ ", imgurl=" + imgurl + "]";
	}
	 
}
