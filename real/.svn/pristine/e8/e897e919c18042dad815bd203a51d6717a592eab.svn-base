<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
<ScrollView 
     android:id="@+id/fs_sv"
    android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fadingEdge="vertical"
        android:scrollbars="vertical">
    <RelativeLayout
        android:id="@+id/adviser"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <RelativeLayout
            android:id="@+id/rl1"
            android:layout_width="match_parent"
            android:layout_height="200dp" >

            <ImageView
                android:id="@+id/portrait"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:scaleType="centerCrop"
                android:src="@drawable/bac" />

            <TextView
                android:id="@+id/name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="30dp"
                android:layout_marginLeft="10dp"
                android:shadowColor="#ff000000"
                android:shadowDx="2"
                android:shadowDy="2"
                android:shadowRadius="5"
                android:text="wew"
                android:textColor="#ffffff"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/serve"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="10dp"
                android:shadowColor="#ff000000"
                android:shadowDx="2"
                android:shadowDy="2"
                android:shadowRadius="3"
                android:text="wew"
                android:textColor="#ffffff"
                android:textSize="16sp" />
        </RelativeLayout>

        <View
            android:id="@+id/div1"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_below="@id/rl1"
            android:layout_centerHorizontal="true"
            android:background="#4fbfbfbf" />

        <LinearLayout
            android:id="@+id/ll1"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_below="@id/div1"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="10dp"
                android:src="@drawable/icon_wechat" />

            <TextView
                android:id="@+id/weixin"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="20dp"
                android:text="fuwu2222"
                android:textColor="#666666"
                android:textSize="16sp" />
        </LinearLayout>

        <View
            android:id="@+id/div2"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_below="@id/ll1"
            android:layout_centerHorizontal="true"
            android:background="#4fbfbfbf" />

        <LinearLayout
            android:id="@+id/ll2"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_below="@id/div2"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="10dp"
                android:src="@drawable/icon_vip_hotline" />

            <TextView
                android:id="@+id/phone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="20dp"
                android:text="fuwu2222"
                android:textColor="#666666"
                android:textSize="16sp" />
        </LinearLayout>

        <View
            android:id="@+id/div3"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_below="@id/ll2"
            android:layout_centerHorizontal="true"
            android:background="#4fbfbfbf" />

        <LinearLayout
            android:id="@+id/ll3"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_below="@id/div3"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="10dp"
                android:text="自我介绍"
                android:textColor="#666666"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/instruction"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="20dp"
                android:text="fuwu2222"
                android:textColor="#af000000"
                android:textSize="16sp" />
        </LinearLayout>

        <View
            android:id="@+id/div4"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_below="@id/ll3"
            android:layout_centerHorizontal="true"
            android:background="#4fbfbfbf" />
          <TextView 
            android:id="@+id/cheaptv"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/div4"
            android:layout_centerHorizontal="true"
            android:text="优惠线路"
            android:textSize="18sp"/>
           <View
            android:id="@+id/div5"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_below="@id/cheaptv"
            android:layout_centerHorizontal="true"
            android:background="#4fbfbfbf" />
    <RelativeLayout 
        android:id="@+id/route1"
        android:layout_below="@id/div5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView 
            android:id="@+id/img1"
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:src="@drawable/cheaproute1"/>
        <TextView 
            android:id="@+id/text1"
            android:layout_below="@id/img1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="14sp"
             android:layout_margin="5dp"
            android:text="@string/trip1"/>
        <TextView 
            android:id="@+id/text11"
            android:layout_below="@id/text1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:textColor="#666666"
            android:text="报名即送三峡特产 明星导游伴您同行"
             android:textSize="12sp"/>
        <LinearLayout 
            android:layout_marginTop="150dp"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#ff6600">
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="￥ 495 起"
                android:layout_margin="10dp"
                android:textColor="@color/white"
                android:textSize="14sp"/>
        </LinearLayout>
    </RelativeLayout>
     <View
            android:id="@+id/div6"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_below="@id/route1"
            android:layout_centerHorizontal="true"
            android:background="#4fbfbfbf" />
     <RelativeLayout 
        android:id="@+id/route2"
        android:layout_below="@id/div6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView 
            android:id="@+id/img2"
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:src="@drawable/cheaproute2"/>
        <TextView 
            android:id="@+id/text2"
            android:layout_below="@id/img2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="14sp"
             android:layout_margin="5dp"
            android:text="@string/trip2"/>
        <TextView 
            android:id="@+id/text22"
            android:layout_below="@id/text2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
              android:textColor="#666666"
            android:text="当地参团 恩施火车站 市区内酒店免费接送"
             android:textSize="12sp"/>
        <LinearLayout 
            android:layout_marginTop="150dp"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#ff6600">
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="¥ 499 起  "
                android:layout_margin="10dp"
                android:textColor="@color/white"
                android:textSize="14sp"/>
        </LinearLayout>
    </RelativeLayout>
     <View
            android:id="@+id/div7"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_below="@id/route2"
            android:layout_centerHorizontal="true"
            android:background="#4fbfbfbf" />
     <RelativeLayout 
        android:id="@+id/route3"
        android:layout_below="@id/div7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView 
            android:id="@+id/img3"
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:src="@drawable/cheaproute3"/>
        <TextView 
            android:id="@+id/text3"
            android:layout_below="@id/img3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="14sp"
             android:layout_margin="5dp"
            android:text="@string/trip3"/>
        <TextView 
            android:id="@+id/text13"
            android:layout_below="@id/text3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
              android:textColor="#666666"
            android:text="无购物 宿特色酒店 高铁往返"
             android:textSize="12sp"/>
        <LinearLayout 
            android:layout_marginTop="150dp"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#ff6600">
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="¥ 1599 起  "
                android:layout_margin="10dp"
                android:textColor="@color/white"
                android:textSize="14sp"/>
        </LinearLayout>
    </RelativeLayout>
     </RelativeLayout>
</ScrollView>
    <RelativeLayout
        android:id="@+id/ll_popup"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:layout_alignParentBottom="true"
        android:background="@color/Blush" >

        <ImageView
            android:id="@+id/close"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_margin="10dp"
            android:src="@drawable/cross" />

        <ImageView
            android:id="@+id/img"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_margin="10dp"
            android:src="@drawable/consult_icon" />

        <TextView
            android:id="@+id/content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_toLeftOf="@id/close"
            android:layout_toRightOf="@id/img"
            android:text="TA即将成为带领你遨游世界的专属旅行顾问，一旦确定就不可更改，是否确定？"
            android:textColor="#666666" />

        <Button
            android:id="@+id/yes"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_below="@id/content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp"
            android:layout_toRightOf="@id/img"
            android:background="@drawable/health_infopop_button_full"
            android:text="确定"
               android:textSize="12sp"
            android:textColor="@color/white" />

        <Button
            android:id="@+id/no"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_below="@id/content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"
            android:layout_toRightOf="@id/yes"
            android:background="@drawable/health_infopop_button_empty"
            android:text="让我想想"
            android:textSize="12sp"
            android:textColor="#666666" />
    </RelativeLayout>

</RelativeLayout>