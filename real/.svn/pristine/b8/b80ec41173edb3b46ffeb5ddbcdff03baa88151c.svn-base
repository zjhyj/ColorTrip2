	public static int calTravels(medal medal){
		int travel=medal.getTracknum();
		if (travel>=100) {
			return 5;
		} else if (travel>=50) {
			return 4;
		}else if (travel>=20) {
			return 3;
		}else if (travel>=5) {
			return 2;
		}else if (travel>=1) {
			return 1;
		}else {
			return 0;
		}
	}
